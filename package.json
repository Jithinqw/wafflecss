{
  "name": "wafflecss",
  "version": "1.0.41",
  "description": "React based component library for common library",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write src/**/*.ts{,x}",
    "unittest": "jest -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-storybook-docs": "build-storybook --docs",
    "storybook-docs": "start-storybook --docs --no-manager-cache",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jithinqw/wafflecss.git"
  },
  "author": "Jithinqw",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jithinqw/wafflecss/issues"
  },
  "homepage": "https://github.com/Jithinqw/wafflecss#readme",
  "dependencies": {
    "react": "^18.1.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/builder-webpack4": "^6.5.5",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack4": "^6.5.5",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/enzyme": "^3.10.12",
    "@types/enzyme-adapter-react-15": "^1.0.6",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.0.9",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "cheerio": "^1.0.0-rc.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-15": "^1.4.3",
    "enzyme-adapter-react-16": "^1.15.6",
    "est": "^0.2.1-alpha",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.0",
    "jest-styled-components": "^7.0.8",
    "prettier": "^2.7.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.2.0",
    "rollup": "^2.74.1",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "sb": "^6.5.5",
    "storybook-addon-performance": "^0.16.1",
    "styled-components": "^5.3.5",
    "ts-jest": "^28.0.5",
    "typescript": "^4.6.4"
  },
  "peerDependency": {
    "react": "^18.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm unittest",
      "lint-staged": {
        "src/**/*.{js,ts,tsx,json}": [
          "prettier --write"
        ]
      }
    }
  }
